<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="344.0" prefWidth="367.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.silesiandevs.fxdomain.client.ClientController">
   <children>
      <GridPane prefHeight="344.0" prefWidth="367.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Phone number" />
            <Label text="Street" GridPane.rowIndex="1" />
            <TextField fx:id="phoneNumber" GridPane.columnIndex="1" />
            <TextField fx:id="streetName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="saveBtn" alignment="CENTER" mnemonicParsing="false" onAction="#saveCustomer" text="Save" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="saveBtn1" alignment="CENTER" mnemonicParsing="false" onAction="#showCustomers" text="show" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
